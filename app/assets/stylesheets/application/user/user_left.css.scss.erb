@import "common";

.columns .user_left {
  width: 590px;
  float: left;

  .user_header {
    height: 68px;
    font-size: 18pt;
    color: $orange;
    padding:5px 20px 5px 30px;
    line-height: 68px;
    background: url("<%= asset_path "static/project_header_bg.png" %>") 0px 0px repeat-x;
  }
  .edit {
    min-height: 425px;
    .title {
      padding-top: 20px;
      color: $black;
      font-size: 18pt;
      text-align: center;
    }
    .box {
      padding: 40px;
      color: $grey;
    }
  }
}
