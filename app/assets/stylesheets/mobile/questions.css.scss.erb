#project_wrap_6 {
  background: url(<%= asset_path 'projects/6/header.png' %>) no-repeat 220px 40px;

  .main {
    margin-top: 120px;
  }
}


#project_wrap_7 {
  background: url(<%= asset_path 'projects/7/banner.png' %>) no-repeat -30px 55px;
  .supplies {
    zoom: 1.0;
    position: absolute;
    z-index: 50;
    .school_supply, .entertainment_supply, .electronic_supply, .sports_supply {
      display: display;
      width: 140px;
      height: 100px;
      float:left;
      margin-top: 50px;
    }
    .school_supply {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat 2px -96px;
    }
    .school_supply.selected, .school_supply.hovered {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat 0px 0px;
    }
    .entertainment_supply {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat -144px -90px;
    }
    .entertainment_supply.selected, .entertainment_supply.hovered {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat -144px 4px;
    }
    .electronic_supply {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat -286px -94px;
    }
    .electronic_supply.selected, .electronic_supply.hovered {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat -286px 0px;
    }
    .sports_supply {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat -426px -94px;
    }
    .sports_supply.selected, .sports_supply.hovered {
      background: url(<%= asset_path 'projects/7/item_icons.png' %>) no-repeat -426px 0px;
    }
    .school_supply_text, .entertainment_supply_text, .electronic_supply_text, .sports_supply_text {
      zoom: 1.0;
      position: absolute;
      display: none;
      padding: 10px;
      color: $black;
      top: 180px;
      left: 0px;
      height: 70px;
      width: 119px;
      font-size: 10pt;
      .text_title {
        font-weight: bold;
      }
    }
    .school_supply_text {
      background: #f56979;
    }
    .entertainment_supply_text {
      background: #fefacc;
      margin-left: 139px;
    }
    .electronic_supply_text {
      width: 121px;
      background: #fcd1bd;
      margin-left: 278px;
    }
    .sports_supply_text {
      width: 111px;
      background: #9ce5e8;
      margin-left: 419px;
    }
  }
  .project_7_progress {
    zoom: 1.0;
    position: absolute;
    z-index: 20;
    top: 182px;
    width: 550px;
    .progress_background {
      height: 43px;
      background: #fcfcfc;
      @include gradient(#fcfcfc, #f4f4f4);
      border: 1px solid #f4f4f4;
    }
    .progress_text.stage_1 {
      left: 139px;
    }
    .progress_text.stage_2 {
      left: 278px;
      width: 141px;
    }

    .progress_text.stage_3 {
      left: 419px;
      width: 131px;
    }
    .progress_text {
      zoom: 1.0;
      position: relative;
      top: -45px;
      color: white;
      background: #e51717;
      height: 44px;
      width: 139px;
      @include gradient(#d22422, #e22422);
      .progress_text_wrapped {
        line-height: 44px;
        font-size: 14px;
        vertical-align: middle;
        text-align: center;
      }
    }
  }
  .main {
    .question_left {
      margin-top: 45px;
    }
  }
  .question_right .tab .project_tag, .question_right .project_tab .project_numbers .project_coordinator .coordinator_label, .question_right .tab_menu a.tab_label.selected, 
  .project_right .tab .project_tag, .project_right .project_tab .project_numbers .project_coordinator .coordinator_label {
    background-color: #e22422;
  }
  .question_right .tab_menu a.tab_label.selected, .question_right .tab_menu {
    border-color: #e22422;
  }
  .question_right .project_tab .project_numbers .project_stats .numbers, .question_right .tab_menu a.tab_label, .question_right .question_tab .block_label, .user_stats_small .stats table .numbers, .user_stats_small .stats table .numbers a, .question_left .content .progress .orange, 
  .project_left .project_info .block_label, .project_right .project_tab .project_numbers .project_stats .numbers {
    color: #e22422;
  }
  .question_right .tab_menu a.tab_label.selected {
    color: $white;
  }
}
#project_block_7 {
  .project_overview .project_equation {
    background-color: #e22422;
  }
}
